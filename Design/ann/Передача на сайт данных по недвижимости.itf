  object StringList: TitsvStringList
    Strings.Strings = (
      'SELECT'
      'P.AP_PREMISES_ID,'
      'P.AP_OPERATION_ID,'
      'P.RELEASE_ID,'
      'P.AP_AGENCY_ID,'
      'P.AP_BUILDER_ID,'
      'P.DELIVERY_DATE,'
      'P.NN,'
      'P.PHONES AS PREMISES_PHONES,'
