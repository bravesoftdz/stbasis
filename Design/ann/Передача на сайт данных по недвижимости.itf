        'LEFT JOIN AP_TYPE_HEAT TH ON P.AP_TYPE_HEAT_ID=TH.AP_TYPE_HEAT_I' +
        'D'
      
        'LEFT JOIN AP_TYPE_PHONE TN ON P.AP_TYPE_PHONE_ID=TN.AP_TYPE_PHON' +
        'E_ID'
      
        'LEFT JOIN AP_TYPE_BALCONY TY ON P.AP_TYPE_BALCONY_ID=TY.AP_TYPE_' +
